		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="767"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="String1024">
		<xsd:annotation>
			<xsd:documentation>Строка(1024)</xsd:documentation>
