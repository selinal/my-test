	</xsd:simpleType>
	<xsd:simpleType name="String32">
		<xsd:annotation>
			<xsd:documentation>Строка(32)</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="32"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="String36">
		<xsd:annotation>
			<xsd:documentation>Строка(36)</xsd:documentation>
