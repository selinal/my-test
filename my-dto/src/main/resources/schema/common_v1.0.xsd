	</xsd:simpleType>
	<xsd:simpleType name="String22">
		<xsd:annotation>
			<xsd:documentation>Строка(22)</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="22"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="String27">
		<xsd:annotation>
			<xsd:documentation>Строка(27)</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="27"/>
