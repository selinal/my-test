<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:oms_common="http://sberbank.ru/oms/common/6.0" targetNamespace="http://sberbank.ru/oms/common/6.0" version="6.0.0.2">
	<!-- common simple types -->
	<!-- simple types -->
	<xsd:simpleType name="Global_ID_Type">
		<xsd:annotation>
			<xsd:documentation>Справочное значение (Long)</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:long"/>
	</xsd:simpleType>
	<xsd:simpleType name="Global_ID_String_Type">
		<xsd:annotation>
			<xsd:documentation>Справочное значение (String)</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string"/>
	</xsd:simpleType>
	<xsd:simpleType name="FormattedDate">
		<xsd:annotation>
			<xsd:documentation>Дата в формате YYYY-MM-DD, YYYY.MM.DD, YYYY/MM/DD, DD/MM/YYYY, DD.MM.YYYY, DD-MM-YYYY</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:length value="10"/>
			<xsd:pattern value="\d{4}-\d{2}-\d{2}"/>
			<xsd:pattern value="\d{4}\.\d{2}\.\d{2}"/>
			<xsd:pattern value="\d{4}/\d{2}/\d{2}"/>
			<xsd:pattern value="\d{2}/\d{2}/\d{4}"/>
			<xsd:pattern value="\d{2}\.\d{2}\.\d{4}"/>
			<xsd:pattern value="\d{2}-\d{2}-\d{4}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="String9">
		<xsd:annotation>
			<xsd:documentation>Строка(9)</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="9"/>
		</xsd:restriction>
