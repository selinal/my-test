	<xsd:simpleType name="String255">
		<xsd:annotation>
			<xsd:documentation>Строка(255)</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="255"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="String767">
		<xsd:annotation>
			<xsd:documentation>Строка(767)</xsd:documentation>
